{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./src",
    "incremental": true,
    "paths": {
			"@modules/*": ["./v2/modules/*"],
			"@config/*": ["./v2/config/*"],
			"@providers/*": ["./v2/providers/*"],
		}
  }
}
